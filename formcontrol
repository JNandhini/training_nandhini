private: System::Void button5_Click(System::Object^  sender, System::EventArgs^  e) {
	
	if (this->comboBox1->Text == "")
	{
		MessageBox::Show("Choose the category");

	}
	else{
		if (this->comboBox1->Text == "Movies")
		{
			MessageBox::Show("Selected Movies");
		
		}
		else if (this->comboBox1->Text == "Geography")
		{
			MessageBox::Show("Selected Geography");
		}
		else if (this->comboBox1->Text == "Sports")
		{
			MessageBox::Show("Selected Sports");
		}
		this->Controls->Clear();
	       levels();
	}

　
}

　
　
 public: System::Void button7_Click(System::Object^  sender, System::EventArgs^  e){
	 if (this->radioButton7->Checked == false && this->radioButton8->Checked == false)
	 {
		 MessageBox::Show("Choose the choice");
	 }
	 else
	 {
		 if (this->radioButton7->Checked == true)
		 {
			 this->Controls->Clear();
			 choice();
			
		 }
		 else if (this->radioButton8->Checked == true)
		 {
			 System::String ^ Choice = radioButton8->Text;

			 char* Levels = (char*)Marshal::StringToHGlobalAnsi(Choice).ToPointer();

			 this->Controls->Clear();
			 game();
			
		 }
	 }
}
public: System::Void button6_Click(System::Object^  sender, System::EventArgs^  e) {

	if (this->radioButton4->Checked == false && this->radioButton5->Checked == false && this->radioButton4->Checked == false)
	{
		MessageBox::Show("Choose the choice");
	}
	else
	{
		if (this->radioButton4->Checked == true)
		{
			System::String ^ Choice = radioButton4->Text;
			char* Levels = (char*)Marshal::StringToHGlobalAnsi(Choice).ToPointer();
			this->Controls->Clear();
			choice();
		}
		else if (this->radioButton5->Checked == true)
		{
		
			System::String ^ Choice = radioButton5->Text;
			char* Levels = (char*)Marshal::StringToHGlobalAnsi(Choice).ToPointer(); 
			this->Controls->Clear();
			game();
		
		}
		else if (this->radioButton6->Checked == true)
		{
			System::String ^ Choice = radioButton6->Text;
			char* Levels = (char*)Marshal::StringToHGlobalAnsi(Choice).ToPointer();

			this->Controls->Clear();
			game();
			
		}
	}
		
		
	

　
}
